<div class="inner cover">
  <h1 class="cover-heading">Welcome <%= current_user.first_name %>,</h1>
  <p class="lead">this is my portfolio site.</p>
  <p class="lead"></p></p>
    <%= link_to "About Me", about_me_path, class: "btn btn-lg btn-secondary home-about" %>
  </p>
<% if current_user != GuestUser %>
  <p class="lead"></p></p>
    <%= link_to "Edit/Delete Your Account", edit_user_registration_path, class: "btn btn-lg btn-secondary home-about" %>
  </p>
<% else %>
  <p class="lead"></p></p>
    <%= link_to "Login", new_user_session_path, class: "btn btn-lg btn-secondary home-about" %>
  </p>
<% end %>
</div>